generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Node {
  id        String   @id @default(cuid())
  name      String   @unique
  status    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([name], name: "name")
}
