generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}

model Node {
  id        String   @id @default(cuid())
  name      String   @unique
  status    Boolean
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([name], name: "name")
}
